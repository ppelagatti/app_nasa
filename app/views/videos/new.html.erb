<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", videos_path  %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_video_path %></li>
    </ul>
  </div>          
  <div class="content">            
    <h2 class="Artist"><%= t("web-app-theme.new", :default => "New")%>  Search</h2>
    <div class="inner">
      <p>Insert query</p>

      <% form_tag "a", :method => 'get' do %>
          <p>
            <%= text_field_tag :search, params[:search] %>
            <%= submit_tag "Search", :name => nil %>
          </p>
      <% end %>


    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>